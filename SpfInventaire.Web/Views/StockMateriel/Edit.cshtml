@using SpfInventaire.Core.DAL.ViewModels
@model StockMaterielFormViewModel

@{
    ViewBag.Title = "Edit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section styles{
    @Styles.Render("~/Content/themes/base/jquery-ui.min.css")
}

<h2>Modification</h2>

@Html.Partial("_FormStock", Model)

<div>
    @Html.ActionLink("Liste des Stocks Matériels", "Index")
</div>

@section Scripts{
    @Scripts.Render("~/bundles/jqueryUi")
    @Scripts.Render("~/bundles/AjaxService")
    @Scripts.Render("~/bundles/TicketForm")

    <script>
        $(document).ready(function () {
            TicketForm = new TicketForm();
        });
    </script>
}